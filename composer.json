{
    "name": "varbox/varbox",
    "description": "VarBox",
    "license": "MIT",
    "homepage": "https://varbox.io",
    "keywords": [
        "laravel",
        "varbox",
        "admin panel",
        "access control",
        "media library",
        "content management",
        "content pages",
        "content menus",
        "custom emails",
        "multi language",
        "languages and translations",
        "static translations",
        "geo location",
        "countries list",
        "countries list",
        "cities list",
        "admin redirects",
        "error reporting",
        "application backups",
        "users and admins",
        "roles and permissions",
        "file upload",
        "duplicate models",
        "model revisions",
        "draft models",
        "custom urls",
        "model slugs",
        "admin form",
        "admin menu",
        "filter records",
        "sort records",
        "order records",
        "preview model",
        "csv export",
        "meta tags"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "andrei.badea@varbox.io"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/auth": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/validation": "^8.0",
        "illuminate/view": "^8.0",
        "intervention/image": "^2.5",
        "kalnoy/nestedset": "^5.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "spatie/laravel-backup": "^6.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "orchestra/testbench-dusk": "^6.0",
        "doctrine/dbal": "^2.10",
        "roquie/laravel-dusk-select2": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Varbox\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Varbox\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Varbox\\VarboxServiceProvider"
            ]
        }
    }
}
